<plugin
	name="priv"
	package="wbs.platform.priv">

	<models>
		<model name="priv"/>
		<model name="privType"/>
	</models>

	<layer name="entity">
		<bean class="model.PrivTypeBuilder"/>
		<bean class="model.PrivTypeSpec"/>
		<bean class="model.PrivTypesBuilder"/>
		<bean class="model.PrivTypesSpec"/>
	</layer>

	<layer name="console">
		<bean class="console.ObjectSummaryPrivPart"/>
		<bean class="console.PrivCheckerImpl"/>
		<bean class="console.PrivDataLoaderImpl"/>
	</layer>

	<console-modules>
		<console-module name="priv"/>
	</console-modules>

</plugin>
