<plugin
	name="priv"
	package="wbs.platform.priv">

	<models>
		<model name="priv"/>
		<model name="privType"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.PrivTypeBuilder"/>
		<bean class="fixture.PrivTypesBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.PrivTypeSpec"/>
		<bean class="metamodel.PrivTypesSpec"/>
	</layer>

	<layer name="console">
		<bean class="console.ObjectSummaryPrivPart"/>
		<bean class="console.PrivDataLoaderImplementation"/>
	</layer>

	<console-modules>
		<console-module name="priv"/>
	</console-modules>

</plugin>
