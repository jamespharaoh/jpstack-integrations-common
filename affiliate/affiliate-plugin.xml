<plugin
	name="affiliate"
	package="wbs.platform.affiliate">

	<models>
		<model name="affiliate"/>
		<model name="affiliateType"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.AffiliateTypeBuilder"/>
		<bean class="fixture.AffiliateTypesBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.AffiliateTypeSpec"/>
		<bean class="metamodel.AffiliateTypesSpec"/>
	</layer>

	<console-modules>
		<console-module name="affiliate"/>
	</console-modules>

</plugin>
