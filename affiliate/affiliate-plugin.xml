<plugin
	name="affiliate"
	package="wbs.platform.affiliate">

	<models>
		<model name="affiliate"/>
		<model name="affiliateType"/>
	</models>

	<layer name="entity">
		<bean class="model.AffiliateTypeBuilder"/>
		<bean class="model.AffiliateTypeSpec"/>
		<bean class="model.AffiliateTypesBuilder"/>
		<bean class="model.AffiliateTypesSpec"/>
	</layer>

	<console-modules>
		<console-module name="affiliate"/>
	</console-modules>

</plugin>
