<console-module
	name="affiliate">

	<console-helper-provider
		object-name="affiliate"
		view-priv="parent">

		<context-stuff
			name="affiliateName"
			field="id"/>

		<priv-key
			name="affiliate.manage"
			delegate="parent"
			priv="manage"/>

		<priv-key
			name="affiliate.messages"
			delegate="parent"
			priv="messages"/>

		<priv-key
			name="affiliate.stats"
			delegate="parent"
			priv="stats"/>

	</console-helper-provider>

	<object-context
		name="affiliate"
		object-name="affiliate">

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="affiliate">

		<id-field/>
		<parent-field/>
		<code-field/>

	</field-set>

</console-module>
