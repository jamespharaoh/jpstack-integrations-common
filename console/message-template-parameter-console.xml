<console-module
	name="message-template-parameter">

	<console-helper-provider
		object-name="messageTemplateParameter"
		view-priv="grandparent:manage">

		<context-stuff
			name="messageTemplateParameterName"
			field="name"/>

		<context-stuff
			name="messageTemplateParameterId"
			field="id"/>

		<context-stuff
			name="messageTemplateEntryTypeId"
			field="messageTemplateEntryType.id"/>

		<run-post-processor
			name="messageTemplateEntryType"/>

	</console-helper-provider>

	<object-context
		name="messageTemplateParameter"
		object-name="messageTemplateParameter">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="messageTemplateParameter">

		<name-field/>
		<description-field/>

		<integer-field
			name="maximumLength"/>

		<yes-no-field
			name="required"/>

	</field-set>

	<field-set
		name="create"
		object-name="messageTemplateParameter">

		<name-field/>
		<description-field/>

		<integer-field
			name="maximumLength"/>

		<yes-no-field
			name="required"/>

	</field-set>

	<field-set
		name="summary"
		object-name="messageTemplateParameter">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<integer-field
			name="maximumLength"/>

		<yes-no-field
			name="required"/>

	</field-set>

	<field-set
		name="settings"
		object-name="messageTemplateParameter">

		<parent-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<integer-field
			name="maximumLength"/>

		<yes-no-field
			name="required"/>

	</field-set>

</console-module>