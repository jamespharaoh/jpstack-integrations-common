<console-module
	name="message-template-set">

	<console-helper-provider
		object-name="messageTemplateSet"
		view-delegate-field="parent"
		view-delegate-priv-code="manage">
		
		<context-stuff
			name="messageTemplateSetName"
			field="name"/>

		<context-stuff
			name="mmessageTemplateSetId"
			field="id"/>

		<context-stuff
			name="messageTemplateDatabaseId"
			delegate="messageTemplateDatabase"
			field="id"/>

		<run-post-processor
			name="messageTemplateDatabase"/>

	</console-helper-provider>

	<object-context
		name="messageTemplateSet"
		object-name="messageTemplateSet">

		<list>

			<object-list-page
				fields-provider="messageTemplateSetFieldsProvider"/>

			<object-create-page
				fields-provider="messageTemplateSetFieldsProvider"/>

		</list>

		<object>
		
			<object-summary-page
				fields-provider="messageTemplateSetFieldsProvider"
				priv-key="messageTemplateDatabase.manage"/>

			<object-settings-page
				fields-provider="messageTemplateSetFieldsProvider"
				priv-key="messageTemplateDatabase.manage"/>
				
			<object-events-page/>

		</object>

	</object-context>

</console-module>