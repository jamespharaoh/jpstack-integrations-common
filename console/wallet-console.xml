<console-module
	name="wallet">

	<console-helper-provider
		object-name="wallet"
		view-priv="parent:manage">

		<context-stuff
			name="walletId"
			field="id"/>

		<context-stuff
			name="walletServiceId"
			field="walletService.id"/>

		<run-post-processor
			name="walletService"/>

	</console-helper-provider>

	<object-context
		name="wallet"
		object-name="wallet">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"
				priv-key="walletService.manage"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="wallet">

		<parent-field/>
		<code-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="wallet">

		<id-field/>
		<parent-field/>
		<code-field/>

	</field-set>

	<field-set
		name="settings"
		object-name="wallet">

		<id-field/>
		<code-field/>

	</field-set>

</console-module>
