<model-meta
	name="user">

	<priv-types>

		<priv-type
			name="Manage"
			description="Full control of a user"
			template="yes"/>

	</priv-types>

	<event-types>

		<event-type
			name="User grant"
			text="%0 granted %1 to %2"/>

		<event-type
			name="User revoke"
			text="%0 revoked %1 from %2"/>

		<event-type
			name="User password reset"
			text="%0 reset password for %1"/>

	</event-types>

</model-meta>