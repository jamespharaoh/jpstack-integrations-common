<console-module
	name="user-online">

	<console-helper-provider
		object-name="userOnline"
		id-key="userId"
		view-priv="user:manage">

		<context-stuff
			name="userOnlineName"
			field="user.username"/>

		<run-post-processor
			name="user"/>

	</console-helper-provider>

	<object-context
		name="userOnline"
		object-name="userOnline">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="userOnline">

		<id-field/>

		<text-field
			name="sessionId"/>

		<timestamp-field
			name="timestamp"/>

	</field-set>

	<field-set
		name="summary"
		object-name="userOnline">

		<id-field/>

		<text-field
			name="sessionId"/>

		<timestamp-field
			name="timestamp"/>

	</field-set>

</console-module>