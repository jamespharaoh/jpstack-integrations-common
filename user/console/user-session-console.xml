<console-module
	name="user-session">

	<console-helper-provider
		object-name="userSession"
		view-priv="user:manage">

		<context-stuff
			name="userSessionName"
			field="id"/>

		<context-stuff
			name="userId"
			field="user.id"/>

		<run-post-processor
			name="user"/>

	</console-helper-provider>

	<object-context
		name="userSession"
		object-name="userSession">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="userSession">

		<id-field/>

		<timestamp-field
			name="startTime"/>

		<timestamp-field
			name="endTime"/>

		<text-field
			name="userAgent"/>

		<object-field
			name="consoleDeployment"/>

	</field-set>

	<field-set
		name="summary"
		object-name="userSession">

		<id-field/>
		<parent-field/>

		<timestamp-field
			name="startTime"/>

		<timestamp-field
			name="endTime"/>

		<text-field
			name="userAgent"/>

		<object-field
			name="consoleDeployment"/>

	</field-set>

</console-module>
