<console-module
	name="queue-item-claim">

	<console-helper-provider
		object-name="queueItemClaim"
		view-delegate-field="greatgrandparent"
		view-delegate-priv-code="manage">

		<context-stuff
			name="queueItemClaimName"
			field="id"/>

		<context-stuff
			name="queueItemId"
			delegate="queueItem"
			field="id"/>

		<run-post-processor
			name="queueItem"/>

		<priv-key
			name="queue.manage"
			delegate="greatgrandparent"
			priv="manage"/>

	</console-helper-provider>

	<object-context
		name="queueItemClaim"
		object-name="queueItemClaim">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="queueItemClaim">

		<object-field
			name="user"
			finder="user"/>

		<object-field
			name="unclaimUser"
			finder="user"/>

		<timestamp-field
			name="startTime"/>

		<timestamp-field
			name="endTime"/>

		<enum-field
			name="status"
			helper="queueItemClaimStatusConsoleHelper"/>

	</field-set>

	<field-set
		name="summary"
		object-name="queueItemClaim">

		<id-field/>

	</field-set>

</console-module>
