<console-module
	name="queue">

	<console-helper-provider
		object-name="queue">

		<context-stuff
			name="queueName"
			field="id"/>

		<priv-key
			name="queue.manage"
			delegate="parent"
			priv="manage"/>

	</console-helper-provider>

	<object-context
		name="queue"
		object-name="queue">

		<list>

			<context-tab-page
				name="listActive"
				tab-label="Active"
				title="Active queues"/>

			<context-tab-action-page
				name="users"
				title="List active users"/>

			<supervisor-page
				title="Queue supervisor"/>

			<context-tab-page
				name="supervisorItems"
				hide-tab="yes"/>

			<section
				name="report"
				label="Reports">

				<object-search-page
					name="queueItem"
					tab-label="Queue items"
					object-type="queueItem"
					search-class="wbs.platform.queue.model.QueueItemSearch"
					search-fields="queue-item-report-search"
					results-fields="queue-item-report-results"/>

				<object-search-page
					name="user"
					tab-label="Users"
					object-type="queueItem"
					search-class="wbs.platform.queue.model.QueueItemSearch"
					search-fields="user-queue-search"
					search-dao-method="searchUserQueueReportIds"
					results-class="wbs.platform.queue.model.UserQueueReport"
					results-fields="user-queue-results"
					results-dao-method="searchUserQueueReports"/>

			</section>

			<context-tab-page
				name="debug"/>

			<context-responder-page
				name="home"/>

			<context-responder-page
				name="filter"/>

			<context-get-action
				name="item"/>

			<context-action
				name="claim"/>

			<context-action
				name="unclaim"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-link
				name="item"
				link="queueItem"
				label="Items"
				priv-key="queue.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="queue">

		<id-field/>
		<parent-field/>
		<code-field/>

		<text-field
			name="backgroundColour"/>

		<text-field
			name="foregroundColour"/>

		<integer-field
			name="defaultPriority"/>

	</field-set>

	<field-set
		name="settings"
		object-name="queue">

		<text-field
			name="backgroundColour"
			nullable="yes"/>

		<text-field
			name="foregroundColour"
			nullable="yes"/>

		<integer-field
			name="defaultPriority"
			nullable="yes"/>

	</field-set>

	<field-set
		name="queue-item-report-search"
		class="wbs.platform.queue.model.QueueItemSearch">

		<object-field
			name="sliceId"
			object-type="slice"
			nullable="yes"/>

		<object-field
			name="parentTypeId"
			object-type="objectType"
			nullable="yes"/>

		<interval-field
			name="createdTime"
			label="Created time"
			nullable="yes"/>

		<object-field
			name="processedUserId"
			object-type="user"
			nullable="yes"/>

		<enum-field
			name="state"
			nullable="yes"/>

	</field-set>

	<field-set
		name="queue-item-report-results"
		object-name="queueItem">

		<object-field
			name="processedUser"
			object-type="user"/>

		<object-field
			name="claimedUser"
			field="queueItemClaim.user"
			object-type="user"/>

		<object-field
			name="object"
			field="greatgrandparent"/>

		<object-field
			name="queue"
			object-type="queue"
			field="grandparent"
			root-field="greatgrandparent"/>

		<object-field
			name="item"
			object-type="queueItem"
			field="this"
			root-field="grandparent"/>

		<timestamp-field
			name="createdTime"
			label="Created"/>

		<timestamp-field
			name="pendingTime"
			label="Pending"/>

		<timestamp-field
			name="processedTime"
			label="Processed"/>

	</field-set>

	<field-set
		name="user-queue-search"
		class="wbs.platform.queue.model.QueueItemSearch">

		<interval-field
			name="createdTime"
			label="Time"/>

	</field-set>

	<field-set
		name="user-queue-results"
		class="wbs.platform.queue.model.UserQueueReport">

		<object-field
			name="user"
			object-type="user"/>

		<integer-field
			name="messageCount"/>

		<timestamp-field
			name="firstMessage"/>

		<timestamp-field
			name="lastMessage"/>

		<seconds-field
			name="timeToProcess"
			format="numeric"/>

	</field-set>

	<field-set
		name="queue-debug-form"
		class="wbs.platform.queue.console.QueueDebugForm">

		<object-field
			name="userId"
			object-type="user"
			nullable="no"/>

		<object-field
			name="sliceId"
			object-type="slice"
			nullable="yes"/>

		<yes-no-field
			name="allItems"
			nullable="no"/>

		<yes-no-field
			name="claimedItems"
			nullable="no"/>

	</field-set>

</console-module>
