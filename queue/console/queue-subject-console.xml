<console-module name="queue-subject">

	<console-helper-provider
		object-name="queueSubject">

		<context-stuff
			name="queueSubjectName"
			field="id"/>

		<context-stuff
			name="queueId"
			delegate="queue"
			field="id"/>

		<run-post-processor
			name="queue"/>

		<!--
		<priv-key
			name="queueSubject.manage"
			delegate="grandparent"
			priv="manage"/>
		-->

	</console-helper-provider>

	<object-context
		name="queueSubject"
		object-name="queueSubject">

		<list>
		</list>

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="queueSubject">

		<id-field/>
		<parent-field/>

		<integer-field
			name="objectId"/>

		<object-field
			name="preferredUser"
			object-type="user"/>

		<integer-field
			name="totalItems"/>

		<integer-field
			name="activeItems"/>

	</field-set>

</console-module>