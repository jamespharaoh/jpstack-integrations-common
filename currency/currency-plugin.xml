<plugin
	name="currency"
	package="currency">

	<dependencies>
		<project name="wbs-platform">
			<plugin name="menu"/>
			<plugin name="platform-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-data name="currency-data"/>
	</sql-scripts>

	<models>
		<model name="currency"/>
	</models>

	<layer name="logic">
		<bean class="logic.CurrencyLogicImpl"/>
	</layer>

	<fixtures>
		<fixture name="currency"/>
	</fixtures>

	<console-modules>
		<console-module name="currency"/>
	</console-modules>

</plugin>
