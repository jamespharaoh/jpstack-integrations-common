<console-module
	name="currency">

	<console-helper-provider
		object-name="currency"
		view-delegate-field="root">

		<priv-key
			name="currency_manage"
			delegate="root"
			priv="manage"/>

		<context-stuff
			name="currencyName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="currency"
		object-name="currency">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="currency">

		<name-field/>
		<description-field/>

		<integer-field
			name="divisions"
			label="Div"/>

		<text-field
			name="prefix"/>

		<text-field
			name="suffix"/>

	</field-set>

	<field-set
		name="create"
		object-name="currency">

		<name-field/>
		<description-field/>

		<integer-field
			name="divisions"
			label="Div"/>

		<text-field
			name="prefix"/>

		<text-field
			name="suffix"/>

	</field-set>

	<field-set
		name="summary"
		object-name="currency">

		<id-field/>
		<parent-field/>
		<code-field/>
		<name-field/>
		<description-field/>

		<integer-field
			name="divisions"
			label="Div"/>

		<text-field
			name="prefix"/>

		<text-field
			name="suffix"/>

	</field-set>

	<field-set
		name="settings"
		object-name="currency">

		<name-field/>
		<description-field/>

		<integer-field
			name="divisions"
			label="Div"/>

		<text-field
			name="prefix"/>

		<text-field
			name="suffix"/>

	</field-set>

</console-module>
