<console-module
	name="api-deployment">

	<console-helper-provider
		object-name="apiDeployment"
		view-priv="root:manage">

		<priv-key
			name="apiDeployment.manage"
			priv="root:manage"/>

		<context-stuff
			name="apiDeploymentName"
			field="name"/>

	</console-helper-provider>

	<object-context
		name="apiDeployment"
		object-name="apiDeployment">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<context-tab-form-actions-page
				name="actions">

				<form-action
					name="restart"/>

			</context-tab-form-actions-page>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="apiDeployment">

		<name-field/>
		<description-field/>

		<text-field
			name="host"/>

		<enum-field
			name="state"/>

		<timestamp-field
			name="stateTimestamp"/>

	</field-set>

	<field-set
		name="create"
		object-name="apiDeployment">

		<name-field/>
		<description-field/>

		<text-field
			name="host"
			nullable="yes"/>

		<text-field
			name="serviceName"
			nullable="yes"/>

	</field-set>

	<field-set
		name="summary"
		object-name="apiDeployment">

		<id-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="host"/>

		<text-field
			name="serviceName"/>

		<enum-field
			name="state"/>

		<timestamp-field
			name="stateTimestamp"/>

	</field-set>

	<field-set
		name="settings"
		object-name="apiDeployment">

		<id-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<text-field
			name="host"
			nullable="yes"/>

		<text-field
			name="serviceName"
			nullable="yes"/>

	</field-set>

</console-module>
