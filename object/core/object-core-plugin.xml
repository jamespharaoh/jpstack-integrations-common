<plugin
	name="object-core"
	package="object.core">

	<sql-scripts>
		<sql-schema name="object-type"/>
		<sql-data name="object-type-data"/>
	</sql-scripts>

	<models>
		<model name="objectType"/>
	</models>

	<layer name="hibernate">
		<bean class="hibernate.HibernateHelperProviderBuilder"/>
	</layer>

	<console-modules>
		<console-module name="object-type"/>
	</console-modules>

</plugin>
