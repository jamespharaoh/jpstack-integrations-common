<plugin
	name="object-core"
	package="object.core">

	<dependencies>
		<project name="wbs-platform">
			<plugin name="platform-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-schema name="object-type"/>
		<sql-data name="object-type-data"/>
	</sql-scripts>

	<models>
		<model name="objectType"/>
	</models>

	<layer name="hibernate">
		<bean class="hibernate.HibernateHelperProviderBuilder"/>
	</layer>

	<fixtures>
		<fixture name="objectType"/>
	</fixtures>

	<console-modules>
		<console-module name="object-type"/>
	</console-modules>

</plugin>
