<console-module
	name="object-type">

	<console-helper-provider
		object-name="objectType"
		view-priv="root:manage">

		<context-stuff
			name="objectTypeName"
			field="description"/>

		<priv-key
			name="objectType.manage"
			priv="root:manage"/>

	</console-helper-provider>

	<object-context
		name="objectType"
		object-name="objectType">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="objectType">

		<id-field/>
		<code-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="objectType">

		<id-field/>
		<code-field/>

	</field-set>

</console-module>
