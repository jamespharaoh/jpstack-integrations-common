<plugin
	name="group"
	package="wbs.platform.group">

	<plugin-dependencies>
		<plugin-dependency name="platform-common"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-schema name="group"/>
		<sql-data name="group-data"/>
	</sql-scripts>

	<models>
		<model name="group"/>
	</models>

	<layer name="console">
		<bean class="console.GroupPrivsAction"/>
		<bean class="console.GroupPrivsPart"/>
	</layer>

	<fixtures>
		<fixture name="group"/>
	</fixtures>

	<console-modules>
		<console-module name="group"/>
	</console-modules>

</plugin>
