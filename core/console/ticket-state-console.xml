<console-module
	name="ticket-state">

	<console-helper-provider
		object-name="ticketState"
		view-priv="parent:manage">

		<context-stuff
			name="ticketStateName"
			field="name"/>

		<context-stuff
			name="ticketStateId"
			field="id"/>

		<context-stuff
			name="ticketManagerId"
			delegate="ticketManager"
			field="id"/>

		<run-post-processor
			name="ticketManager"/>

	</console-helper-provider>

	<object-context
		name="ticketState"
		object-name="ticketState">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"
				priv-key="ticketManager.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="ticketState">

		<name-field/>

	</field-set>

	<field-set
		name="create"
		object-name="ticketState">

		<parent-field/>

		<name-field/>

		<seconds-field
			name="minimum"/>

		<seconds-field
			name="maximum"/>

		<yes-no-field
			name="showInQueue"/>

	</field-set>

	<field-set
		name="summary"
		object-name="ticketState">

		<id-field/>
		<parent-field/>
		<code-field/>

		<name-field/>

		<seconds-field
			name="minimum"/>

		<seconds-field
			name="maximum"/>

		<yes-no-field
			name="showInQueue"/>

	</field-set>

	<field-set
		name="settings"
		object-name="ticketState">

		<parent-field/>

		<name-field/>

		<seconds-field
			name="minimum"/>

		<seconds-field
			name="maximum"/>

		<yes-no-field
			name="showInQueue"/>

	</field-set>

</console-module>