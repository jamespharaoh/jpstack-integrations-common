<console-module
	name="ticket-state">

	<console-helper-provider
		object-name="ticketState"
		view-delegate-field="parent"
		view-delegate-priv-code="manage">
		
		<context-stuff
			name="ticketStateName"
			field="name"/>

		<context-stuff
			name="ticketStateId"
			field="id"/>

		<context-stuff
			name="ticketManagerId"
			delegate="ticketManager"
			field="id"/>

		<run-post-processor
			name="ticketManager"/>

	</console-helper-provider>

	<object-context
		name="ticketState"
		object-name="ticketState">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"
				priv-key="ticketManager.manage"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="ticketState">

		<name-field/>

	</field-set>

	<field-set
		name="create"
		object-name="ticketState">
		
		<parent-field/>
		
		<name-field/>
		
		<enum-field
			name="state"
			nullable="no"
			helper="ticketStateStateConsoleHelper"/>
			
	</field-set>

	<field-set
		name="summary"
		object-name="ticketState">

		<id-field/>
		<parent-field/>
		<code-field/>
		
		<name-field/>		

		<enum-field
			name="state"
			nullable="no"
			helper="ticketStateStateConsoleHelper"/>

	</field-set>

	<field-set
		name="settings"
		object-name="ticketState">

		<parent-field/>
		
		<name-field/>

		<enum-field
			name="state"
			nullable="no"
			helper="ticketStateStateConsoleHelper"/>

	</field-set>

</console-module>