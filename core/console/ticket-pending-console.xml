<console-module
	name="ticket-pending">

	<object-context
		name="ticket.pending"
		bean-name="ticketPending"
		object-name="ticket">

		<object>

			<context-tab-responder-page
				name="history"/>

			<context-action-page
				name="form"/>

		</object>

	</object-context>

	<forms>

		<form
			name="history-ticket"
			object-type="ticket"
			type="read-only">

			<column-fields>

				<code-field/>

				<integer-field
					name="numNotes"/>

				<integer-field
					name="numFields"/>

				<timestamp-field
					name="timestamp"/>

				<object-field
					name="ticketState"
					label="State"
					object-type="ticketState"/>

			</column-fields>

		</form>

		<form
			name="history-note"
			object-type="ticket-note"
			type="read-only">

			<column-fields>

				<index-field/>

				<text-field
					name="noteText"/>

			</column-fields>

		</form>

		<form
			name="history-state"
			object-type="ticket-state"
			type="read-only">

			<column-fields>

				<name-field/>

				<yes-no-field
					name="showInQueue"/>

				<integer-field
					name="minimum"/>

				<integer-field
					name="maximum"/>

			</column-fields>

		</form>

	</forms>

</console-module>