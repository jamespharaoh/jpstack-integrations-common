<console-module
	name="ticket-note">

	<console-helper-provider
		object-name="ticketNote"
		view-delegate-field="grandparent"
		view-delegate-priv-code="manage">

		<context-stuff
			name="ticketNoteName"
			field="id"/>

		<context-stuff
			name="ticketNoteId"
			field="id"/>

		<context-stuff
			name="ticketId"
			delegate="ticket"
			field="id"/>

		<run-post-processor
			name="ticket"/>

	</console-helper-provider>

	<object-context
		name="ticketNote"
		object-name="ticketNote">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="ticketNote">

		<parent-field/>
		<index-field/>

		<text-field
			name="noteText"/>

	</field-set>

	<field-set
		name="create"
		object-name="ticketNote">

		<parent-field/>

		<text-field
			name="noteText"/>

	</field-set>

	<field-set
		name="summary"
		object-name="ticketNote">

		<id-field/>
		<parent-field/>
		<index-field/>

		<text-field
			name="noteText"/>

	</field-set>

	<field-set
		name="settings"
		object-name="ticketNote">

		<id-field/>
		<index-field/>

		<text-field
			name="noteText"/>

	</field-set>

</console-module>