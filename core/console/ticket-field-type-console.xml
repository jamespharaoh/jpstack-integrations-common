<console-module
	name="ticket-field-type">

	<console-helper-provider
		object-name="ticketFieldType"
		view-delegate-field="parent"
		view-delegate-priv-code="manage">
		
		<context-stuff
			name="ticketFieldTypeName"
			field="name"/>

		<context-stuff
			name="ticketFieldTypeId"
			field="id"/>

		<context-stuff
			name="ticketManagerId"
			delegate="ticketManager"
			field="id"/>

		<run-post-processor
			name="ticketManager"/>

	</console-helper-provider>

	<object-context
		name="ticketFieldType"
		object-name="ticketFieldType">

		<list>

			<object-list-page
				fields="list"/>

			<object-create-page
				fields="create"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-settings-page
				fields="settings"
				priv-key="ticketManager.manage"/>
				
			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="ticketFieldType">

		<name-field/>

	</field-set>

	<field-set
		name="create"
		object-name="ticketFieldType">
		
		<parent-field/>
		
		<name-field/>
		
		<yes-no-field
			name="required"/>

		<enum-field
			name="type"
			nullable="no"
			helper="ticketFieldTypeTypeConsoleHelper"/>
					
		<object-field
			name="objectType"
			finder="objectType"/>
			
		<yes-no-field
			name="visible"/>
			
	</field-set>

	<field-set
		name="summary"
		object-name="ticketFieldType">

		<id-field/>
		<parent-field/>
		<code-field/>
		
		<name-field/>	
		
		<yes-no-field
			name="required"/>

		<enum-field
			name="type"
			nullable="no"
			helper="ticketFieldTypeTypeConsoleHelper"/>
			
		<object-field
			name="objectType"
			finder="objectType"/>
			
		<yes-no-field
			name="visible"/>

	</field-set>

	<field-set
		name="settings"
		object-name="ticketFieldType">

		<parent-field/>
		
		<name-field/>
		
		<yes-no-field
			name="required"/>

		<enum-field
			name="type"
			nullable="no"
			helper="ticketFieldTypeTypeConsoleHelper"/>
			
		<object-field
			name="objectType"
			finder="objectType"/>
			
		<yes-no-field
			name="visible"/>

	</field-set>

</console-module>