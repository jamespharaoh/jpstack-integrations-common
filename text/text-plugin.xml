<plugin
	name="text"
	package="wbs.platform.text">

	<sql-scripts>
		<sql-schema name="text"/>
	</sql-scripts>

	<models>
		<model name="text"/>
	</models>

	<layer name="web">
		<bean class="web.TextResponder"/>
	</layer>

	<layer name="console">
		<bean class="console.TextConsoleConfig"/>
		<bean class="console.TextFormFieldNativeMapping"/>
		<bean class="console.TextFormFieldPluginProvider"/>
	</layer>

	<console-modules>
		<console-module name="text"/>
	</console-modules>

</plugin>
