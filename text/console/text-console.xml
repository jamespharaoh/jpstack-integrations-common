<console-module
	name="text">

	<console-helper-provider
		object-name="text"
		cryptor="textCryptor">

		<priv-key
			name="text.manage"
			priv="parent:manage"/>

		<context-stuff
			name="textName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="text"
		object-name="text"
		cryptor="textCryptor">

		<object>

			<object-summary-page
				fields="summary"/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="text">

		<id-field/>

	</field-set>

</console-module>
