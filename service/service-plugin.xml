<plugin
	name="service"
	package="wbs.platform.service">

	<models>
		<model name="service"/>
		<model name="serviceType"/>
	</models>

	<layer name="entity">
		<bean class="model.ServiceTypeBuilder"/>
		<bean class="model.ServiceTypeSpec"/>
		<bean class="model.ServiceTypesBuilder"/>
		<bean class="model.ServiceTypesSpec"/>
	</layer>

	<console-modules>
		<console-module name="service"/>
	</console-modules>

</plugin>
