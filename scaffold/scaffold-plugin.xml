<plugin
	name="scaffold"
	package="scaffold">

	<sql-scripts>
		<sql-schema name="root"/>
		<sql-schema name="slice"/>
	</sql-scripts>

	<models>
		<model name="root"/>
		<model name="slice"/>
	</models>

	<fixtures>
		<fixture name="root"/>
		<fixture name="slice"/>
	</fixtures>

	<console-modules>
		<console-module name="root"/>
		<console-module name="slice"/>
	</console-modules>

</plugin>
