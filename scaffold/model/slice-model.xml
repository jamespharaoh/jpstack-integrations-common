<model-meta
	name="slice"
	type="major">

	<fields>

		<generated-id-field/>
		<code-field/>
		<name-field/>
		<description-field/>
		<deleted-field/>

		<string-field
			name="filter"
			nullable="yes"/>

		<string-field
			name="adminEmail"
			nullable="yes"/>

		<string-field
			name="supervisorConfigNames"
			nullable="yes"/>

	</fields>

	<priv-types>

		<priv-type
			name="Manage"
			description="Full control of a slice"
			template="yes"/>

		<priv-type
			name="Supervisor"
			description="View supervisor reports for a slice"
			template="yes"/>

	</priv-types>

</model-meta>
