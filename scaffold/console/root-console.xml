<console-module
	name="root">

	<console-helper-provider
		object-name="root">

		<priv-key
			name="root.manage"
			priv="manage"/>

		<context-stuff
			name="rootName"
			field="code"/>

	</console-helper-provider>

	<object-context
		name="root"
		object-name="root">

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="root">

		<id-field/>
		<code-field/>

	</field-set>

</console-module>
